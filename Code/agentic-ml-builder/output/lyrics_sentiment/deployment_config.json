{
  "project_name": "lyrics_sentiment",
  "version": "1.0.0",
  "deployment_targets": {
    "local": {
      "enabled": true,
      "port": 8000,
      "workers": 4
    },
    "azure": {
      "enabled": false,
      "resource_group": "usd-res-group",
      "workspace": "agentic-ml-builder-proj",
      "compute_target": "gpu-cluster",
      "instance_type": "Standard_NC6s_v3"
    }
  },
  "model_config": {
    "framework": "pytorch",
    "input_type": "text",
    "task": "regression",
    "metrics": [
      "mean_squared_error",
      "r2_score"
    ]
  },
  "monitoring": {
    "enabled": true,
    "log_level": "INFO",
    "mlflow_tracking": true,
    "tensorboard": true
  }
}